# AI TradeMaestro - Production Environment Variables
# Domain: aitrademaestro.ddns.net
# Copy this file to .env.production and fill in your values

# Database Configuration
POSTGRES_USER=postgres
POSTGRES_PASSWORD=YOUR_SECURE_DATABASE_PASSWORD_HERE
POSTGRES_DB=aitrademaestro

# Redis Configuration
REDIS_PASSWORD=YOUR_SECURE_REDIS_PASSWORD_HERE

# Backend Configuration
SECRET_KEY=YOUR_SECURE_SECRET_KEY_HERE_MINIMUM_32_CHARACTERS
ENVIRONMENT=production

# SSL Configuration
SSL_EMAIL=your-email@example.com

# Optional: API Keys and External Services
# OPENAI_API_KEY=your_openai_key_here
# STRIPE_API_KEY=your_stripe_key_here
# SENDGRID_API_KEY=your_sendgrid_key_here